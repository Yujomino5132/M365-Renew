{
  "name": "m365-renew",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "cf-typegen": "wrangler types",
    "predeploy": "npm install && npm run cf-typegen && npm run prettier && npm run lint && npm run tsc",
    "deploy": "wrangler deploy --dry-run",
    "postdeploy": "wrangler deploy",
    "prettier": "prettier --write .",
    "lint": "eslint --ignore-pattern dist/ --ext .ts,.tsx --fix .",
    "tsc": "tsc --noemit",
    "buildServer": "esbuild src/server.ts --bundle --platform=node --target=node24 --minify --outfile=dist/server.js --sourcemap",
    "dev": "wrangler dev",
    "devRemote": "wrangler dev --remote",
    "test": "vitest --run || 1"
  },
  "dependencies": {
    "chanfana": "^2.8.x",
    "hono": "^4.8.x",
    "zod": "^4.0.x"
  },
  "devDependencies": {
    "@cloudflare/puppeteer": "^1.0.4",
    "eslint": "^9.39.x",
    "eslint-config-prettier": "^10.1.x",
    "eslint-plugin-prettier": "^5.5.x",
    "globals": "^16.5.x",
    "prettier": "^3.6.x",
    "puppeteer": "^24.29.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.x",
    "vitest": "^4.0.7",
    "wrangler": "^4.45.4"
  }
}
